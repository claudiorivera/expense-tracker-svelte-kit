    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "autoprefixer": "10.4.12",
    "cssnano": "5.1.13",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "postcss": "8.4.16",
    "postcss-load-config": "4.0.1",
    "prettier": "2.7.1",
