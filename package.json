		"@sveltejs/kit": "2.8.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/bun": "1.1.13",
		"@types/node": "^22.0.0",
		"autoprefixer": "10.4.20",
		"bits-ui": "^0.21.0",
		"clsx": "^2.1.0",
		"formsnap": "^1.0.0",
		"lucide-svelte": "^0.460.0",
		"mongoose": "8.8.2",
		"postcss": "8.4.49",
		"postcss-load-config": "6.0.1",
		"svelte": "4.2.19",
		"svelte-check": "4.1.0",
