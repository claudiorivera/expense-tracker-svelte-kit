    "bits-ui": "^0.17.0",
    "bun-types": "1.0.27",
    "clsx": "^2.1.0",
    "formsnap": "^0.4.2",
    "lucide-svelte": "^0.321.0",
    "mongoose": "8.1.3",
    "postcss": "8.4.35",
    "postcss-load-config": "5.0.2",
