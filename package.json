		"check:nofix": "pnpm biome check src",
		"check-types": "tsc --noEmit"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@faker-js/faker": "9.2.0",
		"@fontsource/lato": "5.1.0",
