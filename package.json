{
	"name": "expense-tracker-svelte-kit",
	"version": "0.0.1",
	"private": true,
	"packageManager": "pnpm@10.28.2",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check:fix": "biome check --write",
		"clean": "rm -rf node_modules .svelte-kit .turbo .vercel",
		"db:seed": "pnpm with-env tsx src/lib/run-seed.ts",
		"with-env": "dotenvx run --"
	},
	"devDependencies": {
		"@biomejs/biome": "2.4.0",
		"@dotenvx/dotenvx": "1.52.0",
		"@faker-js/faker": "10.3.0",
		"@sveltejs/adapter-vercel": "6.3.1",
		"@sveltejs/kit": "2.52.0",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@tailwindcss/vite": "4.1.18",
		"@types/node": "25.2.3",
		"bits-ui": "2.15.5",
		"clsx": "2.1.1",
		"formsnap": "2.0.1",
		"lucide-svelte": "0.564.0",
		"mongoose": "9.2.1",
		"svelte": "5.51.2",
		"svelte-check": "4.4.0",
		"sveltekit-superforms": "2.29.1",
		"tailwind-merge": "3.4.1",
		"tailwind-variants": "3.2.2",
		"tailwindcss": "4.1.18",
		"tailwindcss-animate": "1.0.7",
		"tsx": "4.21.0",
		"typescript": "5.9.3",
		"vite": "7.3.1",
		"vite-tsconfig-paths": "6.1.1",
		"zod": "4.3.6"
	},
	"dependencies": {
		"class-variance-authority": "0.7.1",
		"lucide-react": "0.564.0"
	}
}
